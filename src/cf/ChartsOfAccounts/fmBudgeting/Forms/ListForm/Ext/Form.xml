				<Field>~List.Субконто1</Field>
				<Field>~List.Субконто2</Field>
				<Field>~List.Субконто3</Field>
				<Field>~List.УчаствуетВРасчетеФинреза</Field>
				<Field>~List.УчетПоПодразделениям</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	ExtDimension1.ExtDimension AS ExtDimension1,
	ExtDimension2.ExtDimension AS ExtDimension2,
	ExtDimension3.ExtDimension AS ExtDimension3,
	fmBudgeting.Ref AS Ref,
	fmBudgeting.Code AS Code,
	fmBudgeting.Description AS Description,
	CASE
		WHEN fmBudgeting.Type = Value(AccountType.Active)
			Then "A"
		Else CASE
				WHEN fmBudgeting.Type = Value(AccountType.Passive)
					Then "P"
				Else "AP"
			END
	END AS Type,
	fmBudgeting.OffBalance AS OffBalance,
	fmBudgeting.Currency AS Currency,
	fmBudgeting.FinancialResults AS FinancialResults,
	fmBudgeting.Parent AS Parent,
	fmBudgeting.DenyUsingInEntries AS DenyUsingInEntries,
	fmBudgeting.Order AS Order,
	fmBudgeting.EnglishDescription AS EnglishDescription,
	fmBudgeting.QuickChoiceCode AS QuickChoiceCode,
	fmBudgeting.AccountingByProjects AS AccountingByProjects
FROM
	ChartOfAccounts.fmBudgeting AS fmBudgeting
		LEFT JOIN (SELECT
			fmBudgetingExtDimensionsTypes.ExtDimensionType.Description + CASE
				WHEN fmBudgetingExtDimensionsTypes.TurnoversOnly
					Then " (turn)"
				Else ""
			END AS ExtDimension,
			fmBudgetingExtDimensionsTypes.Ref AS Ref
		FROM
			ChartOfAccounts.fmBudgeting.ExtDimensionTypes AS fmBudgetingExtDimensionsTypes
		WHERE
			fmBudgetingExtDimensionsTypes.LineNumber = 1) AS ExtDimension1
		ON fmBudgeting.Ref = ExtDimension1.Ref
		LEFT JOIN (SELECT
			fmBudgetingExtDimensionsTypes.Ref AS Ref,
			fmBudgetingExtDimensionsTypes.ExtDimensionType.Description + CASE
				WHEN fmBudgetingExtDimensionsTypes.TurnoversOnly
					Then " (turn)"
				Else ""
			END AS ExtDimension
		FROM
			ChartOfAccounts.fmBudgeting.ExtDimensionTypes AS fmBudgetingExtDimensionsTypes
		WHERE
			fmBudgetingExtDimensionsTypes.LineNumber = 2) AS ExtDimension2
		ON fmBudgeting.Ref = ExtDimension2.Ref
		LEFT JOIN (SELECT
			fmBudgetingExtDimensionsTypes.Ref AS Ref,
			fmBudgetingExtDimensionsTypes.ExtDimensionType.Description + CASE
				WHEN fmBudgetingExtDimensionsTypes.TurnoversOnly
					Then " (turn)"
				Else ""
			END AS ExtDimension
		FROM
			ChartOfAccounts.fmBudgeting.ExtDimensionTypes AS fmBudgetingExtDimensionsTypes
		WHERE
			fmBudgetingExtDimensionsTypes.LineNumber = 3) AS ExtDimension3
		ON fmBudgeting.Ref = ExtDimension3.Ref
</QueryText>
				<MainTable>ChartOfAccounts.fmBudgeting</MainTable>
