	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogAccessGroupsSets.Ref AS Ref,
	CatalogAccessGroupsSets.DeletionMark AS DeletionMark,
	CatalogAccessGroupsSets.Description AS Description,
	CatalogAccessGroupsSets.ForExternalUsers AS ForExternalUsers,
	CatalogAccessGroupsSets.SetItemsType AS SetItemsType,
	VALUETYPE(CatalogAccessGroupsSets.SetItemsType) AS ItemsType,
	CatalogAccessGroupsSets.Hash AS Hash,
	CatalogAccessGroupsSets.NotUsedSince AS NotUsedSince,
	CatalogAccessGroupsSets.User AS User,
	ISNULL(GroupsCountInSets.Count, 0) AS GroupsInSetCount,
	CASE
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.AccessGroups)
			THEN 1
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.UserGroups)
			THEN 2
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.Users)
			THEN 3
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.ExternalUsersGroups)
			THEN 4
		WHEN VALUETYPE(CatalogAccessGroupsSets.SetItemsType) = TYPE(Catalog.ExternalUsers)
			THEN 5
		ELSE 0
	END AS OrderByType,
	CatalogAccessGroupsSets.Predefined AS Predefined,
	CatalogAccessGroupsSets.PredefinedDataName AS PredefinedDataName
FROM
	Catalog.SetsOfAccessGroups AS CatalogAccessGroupsSets
		LEFT JOIN (SELECT
			Folders.Ref AS Set,
			COUNT(Folders.Ref) AS Count
		FROM
			Catalog.SetsOfAccessGroups.Folders AS Folders
		
		GROUP BY
			Folders.Ref) AS GroupsCountInSets
		ON (GroupsCountInSets.Set = CatalogAccessGroupsSets.Ref)</QueryText>
				<MainTable>Catalog.SetsOfAccessGroups</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>OrderByType</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
						</dcsset:item>
						<dcsset:item xsi:type="dcsset:OrderItemField">
							<dcsset:field>Description</dcsset:field>
							<dcsset:orderType>Asc</dcsset:orderType>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
