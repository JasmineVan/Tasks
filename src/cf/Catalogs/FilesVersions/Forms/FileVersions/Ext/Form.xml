				<Field>List.CreationDate</Field>
				<Field>List.DeletionMark</Field>
				<Field>List.Extension</Field>
				<Field>List.FullDescr</Field>
				<Field>List.IsCurrent</Field>
				<Field>List.Owner</Field>
				<Field>List.ParentVersion</Field>
				<Field>List.PictureIndex</Field>
				<Field>List.Ref</Field>
				<Field>List.Size</Field>
				<Field>List.VersionNumber</Field>
				<Field>~List.CurrentPictureIndex</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT ALLOWED
	FilesVersionsOverridable.Code AS Code,
	FilesVersionsOverridable.Size AS Size,
	FilesVersionsOverridable.Comment AS Comment,
	FilesVersionsOverridable.Author AS Author,
	FilesVersionsOverridable.CreationDate AS CreationDate,
	FilesVersionsOverridable.FullDescr AS FullDescr,
	FilesVersionsOverridable.ParentVersion AS ParentVersion,
	CASE
		WHEN FilesVersionsOverridable.DeletionMark
			THEN FilesVersionsOverridable.PictureIndex + 1
		ELSE FilesVersionsOverridable.PictureIndex
	END AS PictureIndex,
	FilesVersionsOverridable.DeletionMark AS DeletionMark,
	FilesVersionsOverridable.Owner AS Owner,
	FilesVersionsOverridable.Ref AS Ref,
	CASE
		WHEN FilesVersionsOverridable.Owner.CurrentVersion = FilesVersionsOverridable.Ref
			THEN TRUE
		ELSE FALSE
	END AS IsCurrent,
	FilesVersionsOverridable.Extension AS Extension,
	FilesVersionsOverridable.VersionNumber AS VersionNumber
FROM
	Catalog.FilesVersions AS FilesVersionsOverridable
WHERE
	FilesVersionsOverridable.Owner = &amp;Owner</QueryText>
