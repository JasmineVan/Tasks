	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT ALLOWED
	CatalogMessagesTemplates.Ref,
	CatalogMessagesTemplates.Description,
	CASE
		WHEN CatalogMessagesTemplates.ForSMSMessages
			THEN &amp;SMSMessage
		ELSE &amp;Email
	END AS TemplateFor,
	CASE
		WHEN CatalogMessagesTemplates.Purpose = ""
			THEN &amp;Common
		ELSE CatalogMessagesTemplates.Purpose
	END AS Purpose,
	CatalogMessagesTemplates.TemplateOwner AS TemplateOwner,
	0 HasFiles
FROM
	Catalog.MessageTemplates AS CatalogMessagesTemplates
WHERE
	(&amp;Purpose = ""
		OR CatalogMessagesTemplates.Purpose = &amp;Purpose)
	AND (CatalogMessagesTemplates.TemplateOwner = Undefined
		OR CatalogMessagesTemplates.TemplateOwner = VALUE(Catalog.MetadataObjectIDs.EmptyRef)
		OR &amp;ShowContextTemplates)</QueryText>
				<MainTable>Catalog.MessageTemplates</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
					<dcsset:itemsViewMode>Normal</dcsset:itemsViewMode>
					<dcsset:itemsUserSettingID>911b6018-f537-43e8-a417-da56b22f9aec</dcsset:itemsUserSettingID>
				</ListSettings>
			</Settings>
		</Attribute>
