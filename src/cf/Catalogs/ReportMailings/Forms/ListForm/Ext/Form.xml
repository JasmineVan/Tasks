	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>List.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	BulkEmails.Ref,
	BulkEmails.DataVersion,
	BulkEmails.DeletionMark,
	BulkEmails.Predefined,
	BulkEmails.Parent,
	BulkEmails.IsFolder,
	BulkEmails.Description,
	BulkEmails.Author,
	BulkEmails.Prepared,
	BulkEmails.Personal,
	BulkEmails.Personalized,
	BulkEmails.ExecuteOnSchedule,
	CASE
		WHEN BulkEmails.ExecuteOnSchedule = TRUE
				AND BulkEmails.Prepared = TRUE
			THEN BulkEmails.SchedulePeriodicity
		ELSE VALUE(Enum.ReportMailingSchedulePeriodicities.EmptyRef)
	END AS SchedulePeriodicity,
	BulkEmails.ScheduledJob,
	CASE
		WHEN BulkEmails.ExecuteOnSchedule = TRUE
				AND BulkEmails.Prepared = TRUE
			THEN 0
		ELSE 1
	END AS SchedulePictureIndex,
	BulkEmails.UseDirectory,
	BulkEmails.Folder,
	BulkEmails.UseNetworkDirectory,
	BulkEmails.NetworkDirectoryWindows,
	BulkEmails.NetworkDirectoryLinux,
	BulkEmails.UseFTPResource,
	BulkEmails.FTPServer,
	BulkEmails.FTPDirectory,
	BulkEmails.FTPPort,
	BulkEmails.FTPPassiveConnection,
	BulkEmails.UseEmail,
	BulkEmails.NotifyOnly,
	BulkEmails.Account,
	BulkEmails.HTMLFormatEmail,
	BulkEmails.EmailTextInHTMLFormat,
	BulkEmails.EmailPicturesInHTMLFormat,
	BulkEmails.EmailText,
	BulkEmails.EmailSubject,
	BulkEmails.MailingRecipientType,
	BulkEmails.RecipientEmailAddressKind,
	BulkEmails.BCC,
	BulkEmails.ReplyToAddress,
	BulkEmails.AddToArchive,
	BulkEmails.ArchiveName,
	State.LastRunStart AS LastRun,
	State.WithErrors,
	CASE
		WHEN State.Executed
			THEN State.LastRunStart
		ELSE State.SuccessfulStart
	END AS SuccessfulStart,
	CASE
		WHEN State.LastRunStart IS NULL 
				OR BulkEmails.DeletionMark = TRUE
			THEN 0
		WHEN State.LastRunStart = &amp;EmptyDate
			THEN 0
		WHEN NOT State.Executed
			THEN 1
		WHEN State.WithErrors
			THEN 1
		ELSE 2
	END AS LastRunPictureIndex,
	CASE
		WHEN State.LastRunStart IS NULL 
				OR BulkEmails.DeletionMark = TRUE
			THEN CAST("" AS STRING(50))
		WHEN State.LastRunStart = &amp;EmptyDate
			THEN &amp;NewStatePresentation
		WHEN NOT State.Executed
			THEN &amp;NotCompletedStatePresentation
		WHEN State.WithErrors
			THEN &amp;CompletedWithErrorsStatePresentation
		ELSE &amp;CompletedStatePresentation
	END AS State
FROM
	Catalog.ReportMailings AS BulkEmails
LEFT JOIN InformationRegister.ReportMailingStates AS State
ON (State.BulkEmail = BulkEmails.Ref)
{WHERE TRUE IN
(
	SELECT TOP 1 TRUE
	FROM
		Catalog.ReportMailings.Reports AS Reports
	WHERE Reports.Ref = BulkEmails.Ref AND Reports.Report = &amp;ReportFilter
)}</QueryText>
				<MainTable>Catalog.ReportMailings</MainTable>
